{
  "compilerOptions": {
    "target": "es2016",
    "module": "es6",
    "outDir": "./build", // Corrected outDir
    "baseUrl": ".", //Updated BaseUrl
    "paths": {
      "*": ["src/*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts"
  ]
}